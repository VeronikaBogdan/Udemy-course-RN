{
  "expo": {
    "name": "meals-app",
    "slug": "meals-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/art-3.png",
    "backgroundColor": "#24180f",
    "splash": {
      "image": "./assets/prev-picture.png",
      "resizeMode": "contain",
      "backgroundColor": "#BBF4E9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
